      <button onclick="pdf('#donut')"       >donut</button>
      <button onclick="pdf('#pie')"         >pie</button>
      <button onclick="pdf('#column')"      >column</button>
      <button onclick="pdf('#boxplot')"     >boxplot</button>
      <button onclick="pdf('#stock-chart')" >stock-chart</button>
