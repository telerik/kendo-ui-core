      <button onclick="pdf('body')">page</button>
      <script>
        function pdf(sel) {
            kendo.drawing.drawDOM($(sel))
            .then(function(group) {
                return kendo.drawing.exportPDF(group);
            })
            .done(function(data){
                kendo.saveAs({ dataURI: data, fileName: "kendo.pdf" });
            });
        }
      </script>
