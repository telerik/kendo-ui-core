
        <div id="example">
            <div class="demo-section k-header" style="width: 400px;">
                <h4>Select date</h4>
                <input id="datetimepicker"/>
            </div>
            <div class="box">                
                <h4>Console log</h4>
                <div class="console"></div>
            </div>
            <script>
                $(document).ready(function() {
                    function onOpen(e) {
                        kendoConsole.log("Open: " + e.view + " view");
                    }

                    function onClose(e) {
                        kendoConsole.log("Close: " + e.view + " view");
                    }

                    function onChange() {
                        kendoConsole.log("Change :: " + kendo.toString(this.value(), 'g'));
                    }

                    $("#datetimepicker").kendoDateTimePicker({
                        change: onChange,
                        close: onClose,
                        open: onOpen
                    });
                });
            </script>
        </div>
        
